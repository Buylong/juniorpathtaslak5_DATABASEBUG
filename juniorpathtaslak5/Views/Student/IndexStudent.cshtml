@model IEnumerable<juniorpathtaslak5.Models.Student>

@{
    ViewData["Title"] = "Students";
}

<h1>Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Date of Birth</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.DateOfBirth.ToShortDateString()</td>
                <td>@student.Age</td>
            </tr>
        }
    </tbody>
</table>
